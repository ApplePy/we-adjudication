<div class="ui form">
  <div class="inline field">
    <label>Course</label>
      {{input size="10" type="text" value = course}}
  </div>

  <div class="inline field">
    <label>Description</label>
      {{textarea cols="6" rows="6" value = description}}
  </div>

  <div class="inline field">
    <label>Units</label> 
    {{input type="number" value = unit}}
  </div>

  <div class="inline field">
    <label>Grade</label>
    {{input type="number" value = grade}}
  </div>

  <div class="inline field">
    <label>From</label>
    {{input type="text" value = from}}
  </div>

</div>